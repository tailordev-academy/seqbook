{
  "name": "seqbook",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://tailordev-academy.github.io/seqbook/",
  "dependencies": {
    "bootstrap": "3",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-persist": "^4.9.1",
    "redux-thunk": "^2.2.0",
    "seq-utils": "^3.0.0",
    "victory": "^0.21.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow-coverage": "flow-coverage-report -i 'src/*.js' -i 'src/**/*.js' -x 'src/**/*.test.js' -x 'src/registerServiceWorker.js' -x 'src/index.js' -t html -t text",
    "prettier": "prettier --single-quote --trailing-comma es5 --write 'src/**/*.js'",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fetch-mock": "^5.12.2",
    "flow-bin": "^0.54.1",
    "flow-coverage-report": "^0.3.0",
    "gh-pages": "^1.0.0",
    "prettier": "^1.6.1",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!<rootDir>/node_modules/"
    ]
  }
}
